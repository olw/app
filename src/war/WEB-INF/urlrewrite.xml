<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule>
       <from>^/material/(.*)$</from>
       <to type="redirect">../#!/resource/$1</to>
    </rule>

    <rule>
       <from>^/sammlung/(.*)$</from>
       <to type="redirect">../#!/collection/$1</to>
    </rule>
</urlrewrite>